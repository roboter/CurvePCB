<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
               xmlns:drawable="clr-namespace:CurvePCB.Maui"
             x:Class="CurvePCB.Maui.MainPage">
    <ContentPage.Resources>
        <drawable:MyGraphicsDrawable x:Key="drawable" x:Name="drawable" />
    </ContentPage.Resources>
    <GraphicsView BackgroundColor="AliceBlue" Drawable="{StaticResource drawable}"
                x:Name="MainGraphicsView"
                  StartInteraction="MainGraphicsView_StartInteraction"
                  EndInteraction="MainGraphicsView_EndInteraction"
                DragInteraction="MainGraphicsView_DragInteraction">
    </GraphicsView>
</ContentPage>

